---
export interface Props {
  icon: any;
  text: string;
  cardheader: string;
}

const { icon, cardheader, text } = Astro.props;
---

<div class="flex flex-col items-start gap-[29px] w-1/5">
  <div class="flex flex-col items-start gap-[29px]">
    <div>
      <img src={icon} alt="code icon" />
    </div>
    <div>
      <h2 class="text-[#E0E7FF] font-inter text-3xl">{cardheader}</h2>
    </div>
  </div>
  <div>
    <p class="text-[#E0E7FF] font-inter text-xs">{text}</p>
  </div>
</div>
